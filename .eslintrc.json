{
  "extends": [
    "standard",
    "plugin:ramda/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "env": {
    "node": true,
    "browser": true,
    "es5": true
  },
  "plugins": [
    "svelte3",
    "ramda"
  ],
  "overrides": [
    {
      "files": [
        "*.svelte"
      ],
      "processor": "svelte3/svelte3",
      "rules": {
        "import/first": "off",
        "no-multiple-empty-lines": "off",
      }
    },
  ]
}